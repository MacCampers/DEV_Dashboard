{
   "name": "laravel/laravel",
   "description": "The Laravel Framework.",
   "keywords": ["framework", "laravel"],
   "license": "MIT",
   "type": "project",
   "require": {
      "php": ">=5.6.4",
      "anhskohbo/no-captcha": "^2.4",
      "barryvdh/laravel-debugbar": "^2.4",
      "barryvdh/laravel-snappy": "^0.4.0",
      "barryvdh/laravel-translation-manager": "^0.4.1",
      "chumper/zipper": "^1.0",
      "codecasts/nusoap-php7": "^0.9.7",
      "dimsav/laravel-translatable": "^8.0",
      "doctrine/dbal": "^2.5",
      "guzzlehttp/guzzle": "^6.3",
      "h4cc/wkhtmltoimage-i386": "0.12.x",
      "h4cc/wkhtmltopdf-i386": "0.12.x",
      "kyslik/column-sortable": "^5.4",
      "laravel/cashier": "~7.0",
      "laravel/framework": "5.5.*",
      "laravel/tinker": "~1.0",
      "league/flysystem-aws-s3-v3": "^1.0",
      "maatwebsite/excel": "^2.1",
      "mcamara/laravel-localization": "^1.2",
      "stripe/stripe-php": "^5.1",
      "webpatser/laravel-uuid": "2.*",
      "webup/laravel-sendinblue": "^1.1",
      "yousign/yousign-api-client": "~1.3"
   },
   "require-dev": {
      "filp/whoops": "~2.0",
      "fzaninotto/faker": "~1.4",
      "laravel/dusk": "^1.1",
      "mockery/mockery": "0.9.*",
      "phpunit/phpunit": "~6.0"
   },
   "autoload": {
      "classmap": [
         "database"
      ],
      "psr-4": {
         "App\\": "app/"
      }
   },
   "autoload-dev": {
      "psr-4": {
         "Tests\\": "tests/"
      }
   },
   "scripts": {
      "post-root-package-install": [
         "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
      ],
      "post-create-project-cmd": [
         "php artisan key:generate"
      ],
      "post-install-cmd": [
         "Illuminate\\Foundation\\ComposerScripts::postInstall",
         "php artisan optimize"
      ],
      "post-update-cmd": [
         "Illuminate\\Foundation\\ComposerScripts::postUpdate",
         "php artisan optimize"
      ],
      "post-autoload-dump": [
         "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
         "@php artisan package:discover"
      ]
   },
   "config": {
      "preferred-install": "dist",
      "sort-packages": true,
      "optimize-autoloader": true
   }
}
